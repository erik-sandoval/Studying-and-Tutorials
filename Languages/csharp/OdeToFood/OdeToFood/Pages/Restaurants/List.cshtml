@page
@using Microsoft.AspNetCore.Mvc.RazorPages
@model OdeToFood.Pages.Restaurants.List

@{

}

<h1>Restaurants</h1>

<table class="table">
	@foreach (var r in Model.Restaurants) {
		<tr>
			<td>@r.Name</td>
			<td>@r.Location</td>
			<td>@r.Cuisine</td>
		</tr>
	}
</table>

<div>@Model.Message</div>